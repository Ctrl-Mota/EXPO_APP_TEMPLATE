{
  "name": "@ctrl-mota/expo-template",
  "version": "1.1.1",
  "description": "Template for custom install of an Expo project",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android --localhost",
    "ios": "expo start --ios --localhost",
    "web": "expo start --web",
    "clear": "expo r -c --localhost",
    "eject": "expo eject",
    "check": "prettier --check \"src/**/*\".{js,jsx,json}",
    "format": "prettier --write \"src/**/*\".{js,jsx,json}"
  },
  "dependencies": {
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.4",
    "expo": "^42.0.0",
    "expo-splash-screen": "~0.11.2",
    "expo-status-bar": "~1.0.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-paper": "^4.9.2",
    "react-native-reanimated": "~2.2.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.4.0",
    "react-native-web": "~0.13.12"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "prettier": "^2.2.1"
  },
  "keywords": [
    "expo",
    "template",
    "expo-template",
    "react-native",
    "react-native-template",
    "prettier",
    "context",
    "paper",
    "theme",
    "dark-theme"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ctrl-Mota/expo-template.git"
  },
  "author": "Lucas Mota",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ctrl-Mota/expo-template/issues"
  },
  "homepage": "https://github.com/Ctrl-Mota/expo-template#readme"
}